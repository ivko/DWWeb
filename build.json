{
	"mainConfigFile": "src/config.js",
	"dir": "release",
	"appDir": "src",
	"baseUrl": "./",
	"optimize": "none",
	"skipDirOptimize": true,
	"optimizeCss": "none",
	"removeCombined": true,
	"findNestedDependencies": true,
	"wrapShim": true,
	"fileExclusionRegExp": "^(\\.|node_modules)$",
	"modules": [{
		"name": "dw/core"
	}, {
		"name": "dw/main",
		"exclude": ["dw/core"]
	}, {
		"name": "dw/widgets/dialog",
		"exclude": ["dw/core"]
	}]
}